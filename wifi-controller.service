[Unit]
Description=Wifi Controller Service

[Service]
Type=simple
WorkingDirectory=/home/pi/Desktop/code/pi-tank/
ExecStart=/usr/bin/git pull && /usr/bin/python /home/pi/Desktop/code/pi-tank01/wifi-controller.py 
Restart=on-failure
RestartSec=30s
Environment=PYTHONUNBUFFERED=1

[Install]
